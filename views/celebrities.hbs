<div class="container mt-3 mb-5">
  <h2 class="p-3 mb-5 border-bottom border-secondary">List of celebreties</h2>
  <div class="pl-3 border-left border-secondary">
    {{#each celebrities}}
    <h4 class="p-2 mr-2">
      <a href="/celebrities/{{this._id}}" class="p-1 text-muted bg-warning">+ {{this.name}}</a>
      <small> {{this.occupation}} </small>
    </h4>
    {{/each}}
  </div>
</div>